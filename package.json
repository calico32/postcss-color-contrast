{
  "name": "postcss-color-contrast",
  "packageManager": "yarn@3.2.2",
  "version": "1.0.1",
  "scripts": {
    "tsc": "tsc --esModuleInterop --moduleResolution node --lib esnext --sourceMap --declaration --strict",
    "clean": "rm -r ./dist || :",
    "cjs": "npm run tsc -- --target es6 --module commonjs --outDir dist/cjs src/cjs.ts nominal.d.ts",
    "esm": "npm run tsc -- --target es2020 --module es2020 --outDir dist/esm src/esm.ts nominal.d.ts",
    "build": "npm run clean && npm run cjs && npm run esm"
  },
  "main": "./dist/cjs/cjs.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/postcss-color-contrast.d.ts",
        "default": "./dist/esm/postcss-color-contrast.js"
      },
      "require": {
        "types": "./dist/cjs/postcss-color-contrast.d.ts",
        "default": "./dist/cjs/postcss-color-contrast.js"
      }
    },
    "./*": {
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*"
    }
  },
  "types": "./dist/esm/esm.d.ts",
  "dependencies": {
    "postcss-value-parser": "^4.2.0"
  },
  "peerDependencies": {
    "postcss": "*"
  }
}
